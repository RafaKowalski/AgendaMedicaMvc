@model IEnumerable<AgendaMedicaMvc.Models.RegistroDeAgenda>

<h2>Registro de Agendas</h2>

<p>
    <a asp-action="Create" class="btn btn-default">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Agenda.Id)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.StatusDaAgenda)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Agenda.Id)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.StatusDaAgenda)
            </td>
        </tr>
}
    </tbody>
</table>
